{
  "name": "olive-reserve",
  "version": "1.0.0",
  "description": "My peachy Nuxt.js project",
  "author": "hikarut",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV='dev' nuxt",
    "local": "cross-env NODE_ENV='local' nuxt",
    "stg": "cross-env NODE_ENV='stg' nuxt",
    "prod": "cross-env NODE_ENV='prod' nuxt",
    "mainte": "cross-env NODE_ENV='maintenance' nuxt",
    "build": "cross-env NODE_ENV='dev' nuxt build",
    "build:local": "cross-env NODE_ENV='local' nuxt build",
    "build:stg": "cross-env NODE_ENV='stg' nuxt build",
    "build:prod": "cross-env NODE_ENV='prod' nuxt build",
    "build:mainte": "cross-env NODE_ENV='maintenance' nuxt build",
    "start": "cross-env NODE_ENV='dev' nuxt start",
    "start:local": "cross-env NODE_ENV='local' nuxt start",
    "generate": "cross-env NODE_ENV='dev' nuxt generate",
    "generate:stg": "cross-env NODE_ENV='stg' nuxt generate",
    "generate:prod": "cross-env NODE_ENV='prod' nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/vuetify": "^0.5.0",
    "caniuse-lite": "^1.0.30000974",
    "cross-env": "^5.2.0",
    "crypto-js": "^3.1.9-1",
    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "vuetify-loader": "^1.2.2",
    "vuex-persistedstate": "^2.5.4",
    "lodash": ">=4.17.13",
    "lodash.template": ">=4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "esm": ">=3.1.0",
    "jest": "^23.6.0",
    "mem": ">=4.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.2",
    "webpack-bundle-analyzer": ">=3.3.2",
    "js-yaml": ">=3.13.1",
    "handlebars": ">=4.0.14"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1",
      "^~~/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "last 2 version",
    "android >= 4.4",
    "IE 11"
  ]
}
